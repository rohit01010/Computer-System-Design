//Chip to find bitwise OR of two 8 bit data lines
CHIP Or8Bit {
IN a[8],b[8];
OUT z[8];
PARTS:
Or(a=a[0],b=b[0],out=z[0]);
Or(a=a[1],b=b[1],out=z[1]);
Or(a=a[2],b=b[2],out=z[2]);
Or(a=a[3],b=b[3],out=z[3]);
Or(a=a[4],b=b[4],out=z[4]);
Or(a=a[5],b=b[5],out=z[5]);
Or(a=a[6],b=b[6],out=z[6]);
Or(a=a[7],b=b[7],out=z[7]);
}