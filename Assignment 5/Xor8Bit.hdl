//Chip to find bitwise xor of two 8 bit data lines
CHIP Xor8Bit {
IN a[8],b[8];
OUT z[8];
PARTS:
Xor(a=a[0],b=b[0],out=z[0]);
Xor(a=a[1],b=b[1],out=z[1]);
Xor(a=a[2],b=b[2],out=z[2]);
Xor(a=a[3],b=b[3],out=z[3]);
Xor(a=a[4],b=b[4],out=z[4]);
Xor(a=a[5],b=b[5],out=z[5]);
Xor(a=a[6],b=b[6],out=z[6]);
Xor(a=a[7],b=b[7],out=z[7]);
}